<p id="notice"><%= notice %></p>

<h1>Tipo Mineracaos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Sigla</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_mineracaos.each do |tipo_mineracao| %>
      <tr>
        <td><%= tipo_mineracao.nome %></td>
        <td><%= tipo_mineracao.sigla %></td>
        <td><%= link_to 'Show', tipo_mineracao %></td>
        <td><%= link_to 'Edit', edit_tipo_mineracao_path(tipo_mineracao) %></td>
        <td><%= link_to 'Destroy', tipo_mineracao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

